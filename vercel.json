{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "devCommand": "cd frontend && npm run dev",
  "installCommand": "cd frontend && npm install",
  "outputDirectory": "frontend/.next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "env": {
    "NEXT_PUBLIC_CONTRACT_ADDRESS": "@basepass_contract_address",
    "NEXT_PUBLIC_STAMP_CONTRACT_ADDRESS": "@stamp_contract_address",
    "NEXT_PUBLIC_RPC_URL": "@rpc_url",
    "NEXT_PUBLIC_CDP_API_KEY": "@cdp_api_key",
    "NEXT_PUBLIC_URL": "@deployment_url"
  }
}

